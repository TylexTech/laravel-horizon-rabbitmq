{
    "name": "tylextech/laravel-horizon-rabbitmq",
    "description": "This package starts worker for rabbitmq with more better stability",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/horizon": "^5.10",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.0",
        "illuminate/queue": "^9.40"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tylextech\\LaravelHorizonRabbitmq\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Muhammad Tayyab",
            "email": "m.tayyab9736@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Tylextech\\LaravelHorizonRabbitmq\\LaravelHorizonRabbitmqServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
